[Unit]
Description=OpenStack Neutron Destroy Patch Ports
After=syslog.target network.target openvswitch.service
Before=neutron-openvswitch-agent.service

[Service]
Type=oneshot
User=neutron
ExecStart=/usr/bin/neutron-destroy-patch-ports --config-file /etc/neutron/plugins/ml2/openvswitch_agent.ini
PrivateTmp=false
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
